{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "ANNOTATION_NORMALISED.schema.json",
  "title": "Normalised VEP TSV row",
  "type": "object",
  "additionalProperties": true,
  "required": ["variant_id", "Consequence", "Gene_symbol", "Transcript"],
  "properties": {
    "variant_id": { "type": "string", "minLength": 1 },
    "Consequence": { "type": "string", "minLength": 1 },
    "Gene_symbol": { "type": "string" },
    "Transcript": { "type": "string" },
    "dbSNP": { "type": "string" },
    "Gene_pheno": { "type": "string" },
    "vep_cache_version": { "type": "string", "pattern": "^[0-9]+$" },
    "plugins_version": { "type": "string" }
  }
}
