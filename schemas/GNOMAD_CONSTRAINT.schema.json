{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "GNOMAD_CONSTRAINT.schema.json",
  "title": "gnomAD Constraint TSV row",
  "type": "object",
  "additionalProperties": true,
  "oneOf": [
    { "required": ["gene_symbol"] },
    { "required": ["transcript"] }
  ],
  "required": ["OE_LOF_UPPER", "pLI"],
  "properties": {
    "gene_symbol": { "type": "string" },
    "transcript": { "type": "string" },
    "OE_LOF_UPPER": { "type": "string", "pattern": "^[0-9]*\\.?[0-9]+$" },
    "pLI": { "type": "string", "pattern": "^[01](\\.\\d+)?$|^0?\\.\\d+$" }
  }
}
